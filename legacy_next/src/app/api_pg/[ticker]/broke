'use client'
import {useSearchParams } from 'next/navigation'
import IndicatorGraph from '@components/visualization/IndicatorGraph'

export default function Page({params: { ticker },}:  {params: { ticker: string}})
{
  const searchParams = useSearchParams()
  const search = searchParams.get('search')
  const searchValue = search ?? 'No Query String Parameter';

  console.log(searchValue)
  
  return (
    <>
      <h1>Big Test: { ticker }</h1>
      <h2>Second Big Test: { searchValue }</h2>
      <IndicatorGraph />
    </>
  )
}


